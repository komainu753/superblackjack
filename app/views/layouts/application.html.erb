<!DOCTYPE html>
<html>
  <head>
    <title>Superblackjack</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <header>
    <div class="header-logo">
      <%= link_to("♠ブラックジャック♥","/")%>
    </div>
    <ul class="loginUser"> 
      <li>ユーザー：<%= session[:user_name]%></li>
      <li>おこづかい：<%= session[:user_money]%></li>
    </ul>
    <div class="logOut">ログアウト</div>
</header>

  <body>
    <%= yield %>
  </body>
</html>
